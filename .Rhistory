library("ggplot2")
library(ggplot2)
library()
# Load the ToothGrowth data
data(ToothGrowth)
str(ToothGrowth)
head(ToothGrowth)
summary(ToothGrowth)
# Perform Exploratory Data Analysis
library(ggplot2)
#Replacing OJ and VC with Orange Juice and Vitamin C
ToothGrowth$supp <- as.character(ToothGrowth$supp)
ToothGrowth$supp[ToothGrowth$supp == "OJ"] <- "Orange Juice"
ToothGrowth$supp[ToothGrowth$supp == "VC"] <- "Vitamin C"
#Plotting
ggplot(ToothGrowth, aes(x=factor(dose), y=len)) +
facet_grid(.~supp) +
geom_boxplot(aes(fill = supp), show_guide = FALSE) +
labs(title="Guinea pig tooth length by dosage for each type of supplement",
x="Dose (mg/day)",
y="Tooth Length")
g <- ggplot(aes(x = supp, y = len), data = ToothGrowth)
g + geom_boxplot(aes(fill=supp)) + xlab("Supplement Type") +
ylab("Tooth Length") + facet_grid(~ dose) + ggtitle("Tooth Length Along Supplement Type")
